{
  "name": "web-stream-csv-toolkit",
  "version": "0.0.0",
  "description": "CSV Toolkit for WHATWG Streams API.",
  "type": "module",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "default": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "vitest",
    "format": "biome format . --write",
    "lint": "biome lint .",
    "check": "biome check . --apply",
    "check:no-apply": "biome check .",
    "build": "tsc -p tsconfig.build.json && biome format lib --write",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamiazya/web-stream-csv-parser.git"
  },
  "keywords": [
    "csv",
    "parser",
    "web-streams",
    "web-streams-api"
  ],
  "author": "Yuki Yamazaki <yuki@kamiazya.tech>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kamiazya/web-stream-csv-parser/issues"
  },
  "homepage": "https://github.com/kamiazya/web-stream-csv-parser#readme",
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@fast-check/vitest": "^0.0.8",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6"
  }
}
